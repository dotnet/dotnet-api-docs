{
  "build": {
    "markdownEngineName": "dfm-latest",
    "content": [
      {
        "files": [
          "api/**/*.yml",
          "api/**/*.md"
        ],
        "exclude": [
          "api/overwrites/**",
          "samples/**/*.*",
          "**/includes/**",
          "***/contributing.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.svg",
          "**/*.jpg",
          "**/*.gif",
          "**/*.bmp"
        ],
        "src": "add",
        "dest": ".",
        "exclude": [
          "metadata/**/*.*"
        ]
      }
    ],
    "overwrite": {
        "files": [
          "api/overwrites/**/*.md",
          "apidoc/*.md"
        ]
      },
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/dotnet/breadcrumb/toc.json",
      "_displayLangs": [
        "csharp"
      ],
      "author": "dotnet-bot",
      "ms.author": "dotnetcontent",
      "manager": "wpickett",
      "searchScope": [".NET"],
      "uhfHeaderId": "MSDocsHeader-DotNet",
      "apiPlatform": "dotnet",
      "_op_documentIdPathDepotMapping": {
        "api/": {
          "depot_name": "VS.core-docs"
        }
      }
    },
    "fileMetadata": {
      "ms.prod": {
        "api/**.yml": ".net"
      },
      "ms.topic": {
        "api/**.yml": "managed-reference"
     },
      "langs": {
        "api/**.yml": ["csharp", "vb", "cpp"],
        "api/System.Convert.yml": ["csharp", "vb", "cpp", "fsharp"]        
      },
      "ROBOTS": {
        "api/**.yml": "NOINDEX, NOFOLLOW"
      }
    },
    "dest": "_site",
    "template": [
      "docs.html"
    ],
    "xref": [
      "_zip/office15.zip",
      "_zip/missingapi.yml"
    ],
    "lruSize": 0
  }
}

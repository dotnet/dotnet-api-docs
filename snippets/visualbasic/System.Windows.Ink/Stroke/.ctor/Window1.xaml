
<Window x:Class="StrokeSnippets_VB.Window1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:my="clr-namespace:StrokeSnippets_VB"
    Title="StrokeSnippets_VB"
    xmlns:cc="controls"
    >
  <DockPanel Name="root">
    <StackPanel Background="DarkSlateBlue">
      <Button Click="button1Click" Name="button1">Remove Touched Stroke Points</Button>
      <Button Click="button2Click" Name="button2">Remove Touched Strokes</Button>
      <Button Click="button3Click" Name="button3">Remove Encircled Strokes</Button>
      <Button Click="button4Click" Name="button4">Add Strokes (default)</Button>
      <Button Click="button5Click" Name="button5">Add Shadowed Strokes</Button>
      <Button Click="button6Click" Name="button6">Fit to curve: </Button>
      <Button Click="ToggleDrawingMode_Click" Name="toggleDrawingMode">Toggle Drawing Mode</Button>
    </StackPanel>
    <!--<cc:MyBorder Background="White" x:Name="border1" />-->
  </DockPanel>
</Window>
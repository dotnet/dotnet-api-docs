<?xml version="1.0" ?>
<!-- <Snippet2> -->

<configuration>
  <system.web>
    <compilation debug="true" />
    <authentication mode="Windows" />
    <customErrors mode="Off" />
    <httpRuntime useFullyQualifiedRedirectUrl="true" />

    <deviceFilters>
      <filter name="isHTML32" 
              compare="PreferredRenderingType" 
              argument="html32" />
      <filter name="isWML11" 
              compare="PreferredRenderingType" 
              argument="wml11" />
      <filter name="isCHTML10" 
              compare="PreferredRenderingType" 
              argument="chtml10" />
    </deviceFilters>

  </system.web>
</configuration>
<!-- </Snippet2> -->

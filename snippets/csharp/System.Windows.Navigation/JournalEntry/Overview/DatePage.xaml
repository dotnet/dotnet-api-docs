<Page x:Class="CSharp.DatePage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    WindowTitle="DatePage"
    >
    <DockPanel>
      <TextBlock Name="textBlockLabel" DockPanel.Dock="Top"></TextBlock>
      <Button Name="removeJournalEntryButton" DockPanel.Dock="Bottom" Click="removeJournalEntryButton_Click">Remove Back Entry</Button>
      <Button Name="addBackEntryButton" Click="addBackEntryButton_Click">Add Back Entry</Button>
    </DockPanel>
</Page>
